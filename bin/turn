#!/usr/bin/env ruby
require_relative '../lib/turn'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

move(board, 4, "X")
move(board, 0, "O")

puts "Welcome to Tic Tac Toe!"

display_board(board)
9.times do
  turn(board)
end

